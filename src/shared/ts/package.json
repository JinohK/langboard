{
  "name": "@langboard/core",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "license": "Elastic License 2.0 (ELv2)",
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint . --ext .ts,.js --ignore-pattern .rollup.cache/ --ignore-pattern node_modules/ --ignore-pattern dist/",
    "prettier": "prettier --write .",
    "start": "node dist/index.js",
    "format": "prettier --write \"{tests,src}/**/*.{js,jsx,ts,tsx,json,md}\" --ignore-path .prettierignore",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "exports": {
    "./*": {
      "import": "./dist/*/index.mjs",
      "require": "./dist/*/index.cjs",
      "types": "./dist/*/index.d.ts"
    },
    "./*/react": {
      "import": "./dist/*/react/index.mjs",
      "require": "./dist/*/react/index.cjs",
      "types": "./dist/*/react/index.d.ts"
    },
    "./global": {
      "import": "./dist/global.d.ts",
      "types": "./dist/global.d.ts"
    }
  },
  "dependencies": {
    "axios": "^1.11.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "prettier": "^3.6.2",
    "rollup": "^4.44.1",
    "rollup-plugin-dts": "^6.2.1",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1"
  },
  "optionalDependencies": {}
}
